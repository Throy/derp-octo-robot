<?xml version="1.0"?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="IndignadoServer.Properties.Settings.IndignadoDBConnectionString" connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=IndignadoDB;Integrated Security=True" providerName="System.Data.SqlClient"/>
    </connectionStrings>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true"/>
                    <serviceDebug includeExceptionDetailInFaults="false"/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <!-- Test service -->
            <service name="IndignadoServer.TestService">
                <endpoint address="" binding="basicHttpBinding" contract="IndignadoServer.ITestService">
                    <identity>
                        <dns value="localhost"/>
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8730/IndignadoServer/TestService/"/>
                    </baseAddresses>
                </host>
            </service>

            <!-- Meetings service -->
            <service name="IndignadoServer.Services.MeetingsService">
                <endpoint address="" binding="basicHttpBinding" contract="IndignadoServer.Services.IMeetingsService">
                    <identity>
                        <dns value="localhost"/>
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8730/IndignadoServer/MeetingsService/"/>
                    </baseAddresses>
                </host>
            </service>

            <!-- MovAdmin service -->
            <service name="IndignadoServer.Services.MovAdminService">
              <endpoint address="" binding="basicHttpBinding" contract="IndignadoServer.Services.IMovAdminService">
                <identity>
                  <dns value="localhost"/>
                </identity>
              </endpoint>
              <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
              <host>
                <baseAddresses>
                  <add baseAddress="http://localhost:8730/IndignadoServer/MovAdminService/"/>
                </baseAddresses>
              </host>
            </service>
          
            <!-- SysAdmin service -->
            <service name="IndignadoServer.Services.SysAdminService">
              <endpoint address="" binding="basicHttpBinding" contract="IndignadoServer.Services.ISysAdminService">
                <identity>
                  <dns value="localhost"/>
                </identity>
              </endpoint>
              <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
              <host>
                <baseAddresses>
                  <add baseAddress="http://localhost:8730/IndignadoServer/SysAdminService/"/>
                </baseAddresses>
              </host>
            </service>

            <!-- NewsResources service -->
            <service name="IndignadoServer.Services.NewsResourcesService">
              <endpoint address="" binding="basicHttpBinding" contract="IndignadoServer.Services.INewsResourcesService">
                <identity>
                  <dns value="localhost"/>
                </identity>
              </endpoint>
              <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
              <host>
                <baseAddresses>
                  <add baseAddress="http://localhost:8730/IndignadoServer/NewsResourcesService/"/>
                </baseAddresses>
              </host>
            </service>
        </services>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
